{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "remix watch",
    "build": "remix build",
    "dev:start": "remix dev",
    "vercel:start": "vercel dev",
    "postinstall": "remix setup node",
    "start:dev": "npm-run-all -p watch:css dev:start",
    "preview": "npm-run-all -s build:css build; vercel",
    "deploy": "npm-run-all -s build:css build; vercel --prod",
    "start:dev:vercel": "npm-run-all -p watch:css vercel:start",
    "watch:css": "tailwindcss --output ./app/styles/tailwind.css --watch",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --minify"
  },
  "dependencies": {
    "@remix-run/react": "^1.0.5",
    "@remix-run/vercel": "^1.0.5",
    "@vercel/node": "^1.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^1.0.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.5",
    "@remix-run/serve": "^1.0.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2",
    "vercel": "^21.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}