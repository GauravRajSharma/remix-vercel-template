{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "remix watch",
    "postinstall": "remix setup node",
    "start": "vercel dev",
    "start:dev": "concurrently \"remix dev\" \"tailwindcss --output ./app/styles/tailwind.css --watch\" ",
    "start:dev:vercel": "concurrently \"vercel dev\" \"remix watch\" \"tailwindcss --output ./app/styles/tailwind.css --watch\" ",
    "preview": "yarn build; tailwindcss --output ./app/styles/tailwind.css --minify; vercel",
    "deploy": "yarn build; tailwindcss --output ./app/styles/tailwind.css --minify; vercel --prod"
  },
  "dependencies": {
    "@remix-run/react": "^0.20.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.0.0-beta.8",
    "remix": "^0.20.1",
    "@remix-run/vercel": "^0.20.1",
    "@vercel/node": "^1.8.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.20.1",
    "@remix-run/serve": "^0.20.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.3.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2",
    "vercel": "^21.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
